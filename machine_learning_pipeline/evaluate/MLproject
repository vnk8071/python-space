name: evaluate_data
conda_env: conda.yml

entry_points:
  main:
    parameters:
      model_export:
        description: Fully-qualified artifact name for the exported model to evaluate
        type: str
      test_data:
        description: Fully-qualified artifact name for the test data
        type: str
    command: >-
      python evaluate.py --model_export {model_export} \
                         --test_data {test_data}
